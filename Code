%----------HEADING----------
\begin{center}
    \textbf{\Huge \scshape [First Name] [Last Name]} \\ \vspace{1pt}
    [City, State/Province, Country] \\ \vspace{1pt}
    \small [Phone Number] $|$ \href{mailto:[Email Address]}{\underline{[Email Address]}} $|$ \href{[LinkedIn/Portfolio URL]}{\underline{[LinkedIn/Portfolio]}}
\end{center}


%-----------EDUCATION-----------
\section{Education}
  \resumeSubHeadingListStart
    \resumeSubheading
      {[University/College Name]}{[City, Country]}
      {[Degree Name]; GPA: [X.X/4.0]}{[Start Date] -- [End Date]}
    \resumeSubheading
      {[High School Name]}{[City, Country]}
      {[Diploma/Certificate Name]}{[Start Date] -- [End Date]}
      \resumeItemListStart
        \resumeItem{\textbf{Relevant Coursework:} [Course 1], [Course 2], [Course 3], [Course 4]}
      \resumeItemListEnd
  \resumeSubHeadingListEnd


